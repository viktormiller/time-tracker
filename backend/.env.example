# Toggl API Token (get from https://track.toggl.com/profile)
TOGGL_API_TOKEN="your_toggl_api_token_here"

# Tempo API Token (get from Jira Tempo settings)
TEMPO_API_TOKEN="your_tempo_api_token_here"

# Authentication Secrets (generate with: openssl rand -hex 64)
JWT_SECRET=your_jwt_secret_here
SESSION_SECRET=your_session_secret_here

# Admin Password Hash (generate with: node -e "const bcrypt = require('bcrypt'); bcrypt.hash('yourpassword', 12, (err, hash) => console.log(hash));")
ADMIN_PASSWORD_HASH=your_bcrypt_hash_here

# Admin User
ADMIN_USER=admin

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# Server Port
PORT=3000

# Database URL
DATABASE_URL="postgresql://timetracker:devpassword@localhost:5432/timetracker"
